      "variables": {
        "init": 0,
        "time": 0,
        "input": "../../data/mix1.xlsx"
      }
    },
    {"name": "Inicio", "description": "Inicialización de datos", "model_path": "models.trees.mixed_models_Spain", "model_class": "MixedModelsSpain", "operation": "INIT", "variables": { "time": 0}},
