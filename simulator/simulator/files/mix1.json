    {
      "name": "Harvest",
      "description": "Corta",
      "model_path": "models.harvest.cut_down_systematic",
      "model_class": "CutDownSystematics",
      "operation": "HARVEST",
      "variables": {
        "time": 0,
        "cut_down": "AREA",
        "volumen": 50,
        "species": 25,
        "volume_target": "plot"
      }
    }
